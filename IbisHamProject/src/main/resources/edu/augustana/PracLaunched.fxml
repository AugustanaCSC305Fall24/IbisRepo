<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<HBox xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.PracticeModeController">
    <children>
        <VBox prefHeight="463.0" prefWidth="166.0">
            <children>
                <Button fx:id="PracFromDict" onAction="#switchToPracPage" text="Practice Home" translateX="16.0" translateY="20.0" />
                <Button fx:id="HomeFromPracMode" mnemonicParsing="false" onAction="#switchtoHomePage" text="Home Page" translateX="45.0" translateY="40.0" />
                <Button fx:id="secondaryButton" onAction="#switchToDictionary" prefHeight="79.0" prefWidth="138.0" text="Morse Code Dictionary" translateX="16.0" translateY="300.0" />
            </children>
        </VBox>
        <VBox alignment="CENTER" prefHeight="463.0" prefWidth="401.0" spacing="20.0">
            <children>
                <Label text="Practice Mode Launched!" />

            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
        </VBox>
        <VBox prefHeight="463.0" prefWidth="180.0">
            <children>
                <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0" translateY="360.0">
                    <children>
                        <Button fx:id="dotButton" mnemonicParsing="false" text="Dot" />
                        <Button fx:id="dashButton" mnemonicParsing="false" text="Dash" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</HBox>
