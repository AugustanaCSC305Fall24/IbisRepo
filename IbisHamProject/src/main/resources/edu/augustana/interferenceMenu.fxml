<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.PracticeModeController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="414.0" prefWidth="612.0">
         <children>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label layoutX="196.0" layoutY="26.0" text="Interference Settings">
                     <font>
                        <Font name="Luminari" size="24.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
            <Pane prefHeight="355.0" prefWidth="612.0">
               <children>
                  <CheckBox fx:id="SolarFlaresButton" layoutX="240.0" layoutY="14.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="130.0" text="Solar Flares">
                     <font>
                        <Font name="Luminari" size="18.0" />
                     </font>
                  </CheckBox>
                  <CheckBox fx:id="StormyWeatherButton" layoutX="225.0" layoutY="69.0" mnemonicParsing="false" text="Stormy Weather">
                     <font>
                        <Font name="Luminari" size="18.0" />
                     </font>
                  </CheckBox>
                  <CheckBox fx:id="MachineInterferButton" layoutX="206.0" layoutY="121.0" mnemonicParsing="false" text="Machine Interference">
                     <font>
                        <Font name="Luminari" size="18.0" />
                     </font>
                  </CheckBox>
                  <CheckBox fx:id="PoorEquipButton" layoutX="225.0" layoutY="175.0" mnemonicParsing="false" text="Poor Equipment">
                     <font>
                        <Font name="Luminari" size="18.0" />
                     </font>
                  </CheckBox>
               </children>
            </Pane>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Button fx:id="backButton" layoutX="263.0" layoutY="31.0" mnemonicParsing="false" onAction="#saveAndBackInterference" text="Save and Exit" />
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</AnchorPane>
